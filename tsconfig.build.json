{  "compilerOptions": {    "declaration": true,    "emitDeclarationOnly": true,    "outDir": "dist",    "jsx": "react-jsx",    "target": "ES2022",    "module": "ESNext",    "moduleResolution": "node",    "strict": true,    "esModuleInterop": true,    "skipLibCheck": true,    "forceConsistentCasingInFileNames": true  },  "include": ["src", "src/types.d.ts"],  "exclude": ["node_modules", "src/main.tsx", "src/App.tsx"]}